@using Tax_Tech.Helpers
@model List<Tax_Tech.ViewModels.DocumentViewModel>


<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-responsive-xl" id="thebasic-datatable2">
    <thead>
        <tr>
            <th>@Tax_Tech.Resources.Resource.Reason</th> 
         </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr> 
                        <td>
                            @item.MOFRejectStatus 
                        </td> 
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td>
                        <h5 class="text-center" colspan="6">@Tax_Tech.Resources.Resource.NoDataAvailable</h5>
                    </td>
                </tr>
            }
        }

    </tbody>
</table>



