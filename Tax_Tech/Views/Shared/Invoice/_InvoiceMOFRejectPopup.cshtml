@model Int64
<div class="modal fade text-left" id="default" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel1">@Tax_Tech.Resources.Resource.RejectedDocuments</h4>
                <button type="button" class="close" data-dismiss="modal" id="addExtraDiscountClose" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body" id="DocList">

            </div>
        </div>
    </div>

</div>
<script src="~/Scripts/Custom/PublicConfig.js"></script>
<script src="~/Scripts/Custom/Invoice.js"></script>
<style>
    .modal-dialog {
        max-width: 50% !important;
    }
</style>
