@model Int32
@using Tax_Tech.Helpers;
@{
    SelectList ItemsNamesList = LookupsHelper.GetLookups().ItemsNamesList(Model);

}
<label class="control-label" for="ItemName">@Tax_Tech.Resources.Resource.ItemName<span class="text-danger">*</span> </label>
@if (ItemsNamesList != null)
{
    @Html.DropDownList("ItemName", ItemsNamesList as SelectList, Tax_Tech.Resources.Resource.ItemName,
    new
    {
        @class = "select2 form-control",
        @style = "width  100%;",
        @Onchange = "GetItemsByID('ItemName','item-code')",
        @tabindex = "7"
    })
}
else
{
    <h5 class="text-danger">@Tax_Tech.Resources.Resource.ApiDown</h5>
}