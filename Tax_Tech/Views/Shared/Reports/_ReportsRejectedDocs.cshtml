@model List<Tax_Tech.ApiModels.RejectedDocumentReport>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-responsive-xl table-bordered" id="thebasic-datatable">
    <thead>
        <tr>
            <th>@Tax_Tech.Resources.Resource.ResultType</th>
            <th>@Tax_Tech.Resources.Resource.DocCount</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.MOFRejectStatus</td>
                        <td>@item.DocCount</td>
                    </tr>
                }
                <tr style="font-weight: bolder">
                    <td>@Tax_Tech.Resources.Resource.Total</td>
                    <td>@Model.Sum(i => i.DocCount)</td>
                </tr>
            }
            else
            {
                <tr>
                    <td colspan="8">
                        <h5 class="text-center">@Tax_Tech.Resources.Resource.NoDataAvailable</h5>
                    </td>
                </tr>
            }
        }

    </tbody>
</table>