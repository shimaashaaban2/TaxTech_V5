@model List<Tax_Tech.ApiModels.DocumentTotalReport>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-hover table-responsive-xl table-bordered" id="thebasic-datatable">
    <thead>
        <tr>
            <th>@Tax_Tech.Resources.Resource.Status</th>
            <th>@Tax_Tech.Resources.Resource.DocCount</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.ProcessStatusTitle</td>
                    <td>@item.DocCount</td>
                </tr>

            }
            <tr style="font-weight: bolder">
                <td>@Tax_Tech.Resources.Resource.TotalCount</td>
                <td>@Model.Sum(i => i.DocCount)</td>
            </tr>
        }
    </tbody>
</table>