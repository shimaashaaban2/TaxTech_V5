@model IEnumerable<Tax_Tech.ApiModels.EReceiptApi.BillingLogsModel>


<section id="basic-datatable">
    <div class="row">
        <div class="content-body col-md-12">
            <div class="card">
                <div class="card-datatable">
                    <table class="datatables-basic table table-striped table-responsive table-bordered" id="thebasic-datatable">
                        <thead>
                            <tr>
                                
                                <th>@Tax_Tech.Resources.Resource.LogDatetime</th>
                                <th>@Tax_Tech.Resources.Resource.EReceiptNumber</th>
                                <th>@Tax_Tech.Resources.Resource.RequestSuccess</th>
                                <th>@Tax_Tech.Resources.Resource.RequestDatetime</th>
                                <th>@Tax_Tech.Resources.Resource.Doing</th>
                                <th>@Tax_Tech.Resources.Resource.CountOfReceipt</th>
                                <th>@Tax_Tech.Resources.Resource.ResultMsg</th>

                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null && Model.Count() > 0)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                        @*<td>@item.LogID</td>*@
                                        <td>@item.LogDatetime</td>
                                        <td>@item.EReceiptNumber</td>
                                        <td>@item.RequestSuccess</td>
                                        <td>@item.RequestDatetime</td>
                                        <td>@item.Doing</td>
                                        <td>@item.CountOfReceipt</td>
                                        <td>@item.ResultMsg</td>

                                    </tr>
                                }
                            }
                            else
                            {
                                <tr>
                                    <td colspan="8">
                                        <h5 class="text-center">@Tax_Tech.Resources.Resource.NoDataAvailable</h5>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
           </div>
     </div>
   </div>
</section>
