@*@model  Tax_Tech.ApiModels.EReceiptApi.ReceiptListModel*@

<div class="modal fade text-left" id="default" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel1">@Tax_Tech.Resources.Resource.EReceiptQRCode</h4>
                <button type="button" class="close" data-dismiss="modal" id="addExtraDiscountClose" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <span style="width: 200px !important;"><span style="color: #2196F3;">@Tax_Tech.Resources.Resource.UUID</span>: @ViewBag.UUID </span>
                    </div>

                   
                    <div class="col-md-12">
                        <div class="form-group d-flex justify-content-center">

                            <img src="@ViewBag.QrCodeUri" width="250" height="250" class="img-thumbnail" />
                        </div>
                    </div>


                </div>
                </div>
            </div>
        </div>
    </div>


