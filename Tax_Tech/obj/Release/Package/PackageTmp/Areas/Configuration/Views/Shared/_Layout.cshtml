        <script>
            window.addEventListener('load', function () {
                if (location.href.includes('Home')) {
                    $.app.nav.navItem[0].classList.add('active');
                }
                else if (localStorage.getItem('current-nav-item')) {
                    let index = localStorage.getItem('current-nav-item');
                    if (localStorage.getItem('has-sub')) {
                        $.app.nav.navItem[localStorage.getItem('has-sub')].classList.add('open');
                    }
                    $.app.nav.navItem[parseInt(index)].classList.add('active');
                }
                [...$.app.nav.navItem].forEach((e, i) => {
                    if (!e.className.includes('has-sub')) {
                        e.onclick = function () {
                            [...$.app.nav.navItem].forEach((e, i) => {
                                e.classList.remove('active');
                            });
                            localStorage.setItem('current-nav-item', i);
                            e.classList.add('active');
                            localStorage.removeItem('has-sub');
                        }
                    }
                    else {
                        e.onclick = function () {
                            localStorage.setItem('has-sub', i);
                        }
                    }
                });
            });
        </script>
