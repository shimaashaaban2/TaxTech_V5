        <script>
            window.addEventListener('load', function () {
                if (location.href.includes('Home')) {
                    $.app.nav.navItem[0].classList.add('active');
                }
                else if (localStorage.getItem('config-current-nav-item')) {
                    let index = localStorage.getItem('config-current-nav-item');
                    if (localStorage.getItem('config-has-sub')) {
                        $.app.nav.navItem[localStorage.getItem('config-has-sub')].classList.add('open');
                    }
                    $.app.nav.navItem[parseInt(index)].classList.add('active');
                }
                [...$.app.nav.navItem].forEach((e, i) => {
                    if (!e.className.includes('config-has-sub')) {
                        e.onclick = function () {
                            [...$.app.nav.navItem].forEach((e, i) => {
                                e.classList.remove('active');
                            });
                            localStorage.setItem('config-current-nav-item', i);
                            e.classList.add('active');
                            localStorage.removeItem('config-has-sub');
                        }
                    }
                    else {
                        e.onclick = function () {
                            localStorage.setItem('config-has-sub', i);
                        }
                    }
                });
            });
        </script>
